{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "import/extensions": [
            "error",
            "always"
        ],
        "import/prefer-default-export": "off",
        "no-use-before-define": "off"
    }
}